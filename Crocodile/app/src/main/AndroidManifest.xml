<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crocodile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Crocodile">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CommandsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Activities.RulesActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Activities.OptionsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Activities.ScoreActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Activities.PlayActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Activities.ResultActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>